<script type="text/template" id="log-template">
<%= stylesheet_link_tag "style-log.css" %>

<div class="container-1">
	<h3> User</h3>
	<% @users.each do |user| %>
		<div class="container-2">
			<div> <%= user.name %> </div>
			<div> <%= user.avatar %> </div>
			<div> <%= user.current_status %> </div>
			<div> <%= user.points %> </div>
			<div> <%= user.is_admin %> </div>
		</div>
	<% end %>
</div>
</script>

