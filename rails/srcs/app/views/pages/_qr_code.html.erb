<script type="text/template" id="log-qrcode">
<%= stylesheet_link_tag "style-qrcode.css" %>

## This is your QR code
<table class="qr">
  <% @qr.modules.each_index do |x| %>
      <tr>
        <% @qr.modules.each_index do |y| %>
            <% if @qr.qrcode.checked?(x,y) %>
                <td class="black"/>
            <% else %>
                <td class="white"/>
            <% end %>
        <% end %>
      </tr>
  <% end %>
</table>
