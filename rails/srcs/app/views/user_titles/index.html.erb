<p id="notice"><%= notice %></p>

<h1>User Titles</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Tournament</th>
      <th>Title</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_titles.each do |user_title| %>
      <tr>
        <td><%= user_title.user_id_id %></td>
        <td><%= user_title.tournament_id %></td>
        <td><%= user_title.title %></td>
        <td><%= user_title.status %></td>
        <td><%= link_to 'Show', user_title %></td>
        <td><%= link_to 'Edit', edit_user_title_path(user_title) %></td>
        <td><%= link_to 'Destroy', user_title, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Title', new_user_title_path %>
